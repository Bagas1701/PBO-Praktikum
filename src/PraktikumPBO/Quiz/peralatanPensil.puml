@startuml Alat Tulis
package PraktikumPBO.Quiz {
    class AlatTulis {
        - nama : String
        - stok : int
        - hargaSatuan : int
        - hargaTotal : int

        + setNama(nama : String) : void
        + setStok(stok : int) : void
        + setHargaSatuan(hargaSatuan : int) : void
        + getNama() : String
        + getStok() : int
        + getHargaSatuan() : int
        + getHargaTotal() : int
        + tampilkanInfo() : void
    }

    class MainAlatTulis {
        + main(args : String[]) : void
    }

    MainAlatTulis --> AlatTulis : "membuat objek"
}
